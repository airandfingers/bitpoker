<% include head_start %>
        <link type="text/css" rel="stylesheet" href="/css/pokertable.css" />
    <% include head_end_and_header %>

        <div id="pokerTableWrapper">
            <div id="pokerCanvasDiv">
                    <canvas id="canvas" width="690" height="480" style= 'z-index :0'></canvas>
                   </div> <!-- pokerCanvasDiv-->
            <div id="cashierDiv">
                <form id = 'cashier' style='display: none'>    

  <div id="maxDiv">
                       <input type = 'radio' id ="maxRadio" name = 'addChipsRadio' value="max" size="-4">
                    <p id = 'maxText'>Max:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    &nbsp;</p>
                    <input type = 'text' id="maxAmount">

</div> <!-- maxDiv-->

<div id="otherAmountDiv">
            <input type = 'radio' id ="otherAmountRadio" name = 'addChipsRadio' >
                <p id = 'otherAmountText'>    Other Amount: </p>
                    <input type = 'text' id="otherAmount"> 

</div> <!-- otherAmountDiv-->

<div id="autoRebuyDiv">

                      <input type = 'radio' id ="autoRebuyRadio" name = 'addChipsRadio' >
                 <p id = 'autoRebuyText'>   Auto-Rebuy to: </p>
                    <input type = 'text' id="autoRebuyAmount" >     </form>  

</div> <!-- autoRebuyDiv-->

            </div> <!-- cashier-->
            <div id="chatDiv">
                <form id='chatForm'>
           <input type = 'text' id="chat">
                <input type = 'text' id="trolol" style ="display :none">
                </form>
             </div> <!-- chatDiv-->

             <!-- bet size DIV-->
            <div id="betSizeDiv">
                <form id='betSizeForm'>
           <input type = 'text' id="betSize">
                <input type = 'text' id="trololol" style ="display :none">
                </form>
             </div> <!-- bet size DIV-->
               
        </div> <!-- poker table wrapper -->
            <!--include values to expose to scripts-->

            <input type="hidden" id="server_values" data-table_state="<%= table_state %>" data-table_state_url="/table_state/<%= table_id %>" data-room_state="<%= room_state %>" />


        <!--include values to expose to scripts-->
